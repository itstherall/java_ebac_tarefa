package parteDois;

public class Pessoa {
	String nome = new String();
	String genero = new String();
	
	public Pessoa(String nome, String genero) {
		super();
		this.nome = nome;
		this.genero = genero;
	}
	
	
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getGenero() {
		return genero;
	}
	public void setGenero(String genero) {
		this.genero = genero;
	}
	
	
}
